  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:style": "stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-actions-formatters",
    "lint:style:fix": "stylelint '**/*.scss' --fix",
    "lint:ts": "glint",
    "start": "ember serve",
    "test:only": "ember exam --random ''",
    "test:server": "ember exam -s --random",
    "test:parallel": "ember exam --split=`getconf _NPROCESSORS_ONLN` --parallel=1 --random",
    "test:parallel:dot": "ember exam --split=`getconf _NPROCESSORS_ONLN` --parallel=1 --reporter=dot --random",
    "test:ember": "ember test",
    "g": "EMBER_TYPESCRIPT_BLUEPRINTS=true ember g"
  },
